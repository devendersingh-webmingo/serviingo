<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@id/rl_topp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:layout_margin="@dimen/_2sdp"
        android:background="@color/head_lightBlue">


        <ImageView
            android:id="@+id/arrowIvv"

            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="@dimen/_5sdp"
            android:rotation="-90"
            android:src="@drawable/arrow"
            app:tint="@color/white" />

        <TextView
            android:id="@+id/tv_dashboardd"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_40sdp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/_25sdp"
            android:layout_toRightOf="@id/arrowIvv"
            android:background="@color/head_lightBlue"
            android:fontFamily="@font/roboto"
            android:gravity="center"

            android:paddingLeft="@dimen/_15sdp"
            android:text="Dashboard > Referral "
            android:textColor="@color/white"
            android:textSize="@dimen/_14sdp" />

    </RelativeLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/root_lay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_topp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="@dimen/_2sdp"
            android:paddingRight="@dimen/_2sdp">

            <RelativeLayout
                android:id="@+id/Rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="0dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="0dp">

                    <ImageView
                        android:id="@+id/image"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_200sdp"
                        android:background="@drawable/referal"
                        android:src="@drawable/earn" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/referRL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Rl"
                    android:layout_marginTop="@dimen/_160sdp"
                    android:padding="0dp"
                    tools:ignore="NotSibling">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/cv_View"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_20sdp"
                        android:padding="@dimen/_10sdp"

                        app:cardBackgroundColor="#fafafa"

                        app:cardCornerRadius="@dimen/_12sdp"
                        app:cardElevation="@dimen/_10sdp"
                        app:contentPadding="0dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_2sdp"
                            android:gravity="center"

                            android:padding="@dimen/_5sdp">

                            <ImageView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="@dimen/_30sdp"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:src="@drawable/wallet" />

                            <TextView
                                android:id="@+id/CateName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_2sdp"
                                android:gravity="center"
                                android:padding="@dimen/_5sdp"
                                android:text="@string/share_your_referral_and_invite_your_friends_you_earn_u20b9_100_of_ervery_refferal_s_cashback"
                                android:textColor="@color/textcolor"
                                android:textSize="@dimen/_13sdp"
                                android:textStyle="bold" />
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/copyRL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/referRL"
                    tools:ignore="NotSibling">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:layout_marginBottom="@dimen/_8sdp"
                        android:background="@color/background"
                        android:padding="@dimen/_10sdp"

                        app:cardCornerRadius="@dimen/_12sdp"
                        app:cardElevation="@dimen/_10sdp"
                        app:contentPadding="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="@dimen/_5sdp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_margin="@dimen/_2sdp"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"


                                android:padding="@dimen/_5sdp"
                                android:text="Your Referral Code "
                                android:textColor="@color/black"
                                android:textSize="@dimen/_15sdp"
                                android:textStyle="bold" />

                            <androidx.cardview.widget.CardView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                app:cardBackgroundColor="#F2F2F2"

                                app:cardCornerRadius="@dimen/_5sdp"
                                app:cardElevation="@dimen/_10sdp"
                                app:contentPadding="5dp">

                                <TextView
                                    android:id="@+id/referTV"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_margin="@dimen/_2sdp"
                                    android:gravity="center"
                                    android:padding="@dimen/_5sdp"
                                    android:text="https://docs.google.com/"
                                    android:textColor="@color/textcolor"
                                    android:textSize="@dimen/_10sdp"
                                    android:textStyle="bold" />
                            </androidx.cardview.widget.CardView>

                            <LinearLayout
                                android:layout_marginTop="@dimen/_15sdp"

                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="horizontal"
                            >



                                <androidx.cardview.widget.CardView
                                    android:layout_margin="@dimen/_5sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:cardBackgroundColor="#03A9F4"

                                    app:cardCornerRadius="@dimen/_8sdp"
                                    app:cardElevation="@dimen/_10sdp"
                                    app:contentPadding="5dp">

                                    <TextView
                                        android:id="@+id/CopyLinkTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="Copy code"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/_12sdp"
                                        android:textStyle="bold" />
                                </androidx.cardview.widget.CardView>

                                <androidx.cardview.widget.CardView
                                    android:id="@+id/sharingcard"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:cardBackgroundColor="#03A9F4"
                                    android:layout_margin="@dimen/_5sdp"

                                    app:cardCornerRadius="@dimen/_8sdp"
                                    app:cardElevation="@dimen/_10sdp"
                                    app:contentPadding="5dp">
                                    <LinearLayout

                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:orientation="horizontal"
                                        >

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="Share"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/_12sdp"
                                        android:textStyle="bold" />
                                        <ImageView
                                            android:layout_marginRight="@dimen/_5sdp"
                                            android:layout_width="25dp"
                                            android:layout_height="@dimen/_25sdp"
                                            android:layout_gravity="center"
                                            android:gravity="center"
                                            android:src="@drawable/ic_share"

                                            app:tint="@color/white" />
                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                            </LinearLayout>



                            <LinearLayout

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="fill_parent"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView

                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/roboto"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="Total Referrals"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_15sdp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/totalreferTv"
                                        android:layout_width="@dimen/_100sdp"

                                        android:layout_height="@dimen/_35sdp"
                                        android:layout_gravity="center"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:background="@drawable/allsideborder"
                                        android:fontFamily="@font/roboto_bold"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="0"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_13sdp"
                                        android:textStyle="bold" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="fill_parent"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:fontFamily="@font/roboto"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="Amount Earned"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_15sdp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/AmountTv"
                                        android:layout_width="@dimen/_100sdp"

                                        android:layout_height="@dimen/_35sdp"
                                        android:layout_gravity="center"
                                        android:layout_margin="@dimen/_2sdp"
                                        android:background="@drawable/allsideborder"
                                        android:fontFamily="@font/roboto_bold"
                                        android:gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:text="\u20B9 0"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_13sdp"
                                        android:textStyle="bold" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </RelativeLayout>

                <RelativeLayout

                    android:id="@+id/recycler_viewRl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/copyRL"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp"
                    android:layout_marginBottom="@dimen/_8sdp"
                    tools:ignore="NotSibling">

                    <TextView

                        android:id="@+id/ReferralSummary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:fontFamily="@font/roboto"
                        android:padding="@dimen/_10sdp"
                        android:text="Referral Summary"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_15sdp"
                        android:textStyle="bold" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recycler_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/ReferralSummary"
                        android:layout_marginTop="0dp"
                        android:background="#F1F1F1"
                        tools:itemCount="3"
                        tools:listitem="@layout/referraluseradapter" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/howitswork"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/recycler_viewRl"
                    android:layout_alignParentBottom="true"
                    tools:ignore="NotSibling">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"

                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:layout_marginBottom="@dimen/_30sdp"
                        android:background="@color/background"
                        android:padding="@dimen/_10sdp"

                        app:cardCornerRadius="@dimen/_12sdp"
                        app:cardElevation="@dimen/_10sdp"
                        app:contentPadding="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="@dimen/_5sdp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left"
                                android:layout_margin="@dimen/_2sdp"
                                android:fontFamily="@font/roboto_bold"
                                android:gravity="left"


                                android:padding="@dimen/_5sdp"
                                android:text="How It works"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_18sdp"
                                android:textStyle="bold" />

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:id="@+id/LL"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:background="@drawable/circleblue"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/ivImages"
                                        android:layout_width="@dimen/_45sdp"
                                        android:layout_height="@dimen/_45sdp"
                                        android:layout_alignParentLeft="true"
                                        android:layout_gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/friend" />
                                </LinearLayout>


                                <TextView
                                    android:id="@+id/choosefile"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_toRightOf="@+id/LL"
                                    android:fontFamily="@font/roboto_bold"
                                    android:padding="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_5sdp"

                                    android:text="Invite Friends"
                                    android:textColor="#232323"
                                    android:textSize="@dimen/_15sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:id="@+id/newText"
                                    android:layout_width="match_parent"

                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/choosefile"
                                    android:layout_toRightOf="@+id/LL"
                                    android:fontFamily="@font/roboto"
                                    android:padding="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_5sdp"
                                    android:text="Invite your friends throught your  refferal link"
                                    android:textColor="#232323"
                                    android:textSize="@dimen/_12sdp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp">

                                <LinearLayout
                                    android:id="@+id/LLL"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:background="@drawable/circleblue"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/ivImagess"
                                        android:layout_width="@dimen/_45sdp"
                                        android:layout_height="@dimen/_45sdp"
                                        android:layout_alignParentLeft="true"
                                        android:layout_gravity="center"
                                        android:padding="@dimen/_5sdp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/file" />
                                </LinearLayout>


                                <TextView
                                    android:id="@+id/choosefilee"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_toRightOf="@+id/LLL"
                                    android:fontFamily="@font/roboto_bold"
                                    android:padding="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_5sdp"
                                    android:text="Placed First Booking"

                                    android:textColor="#232323"
                                    android:textSize="@dimen/_15sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:id="@+id/newTextt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/choosefilee"
                                    android:layout_toRightOf="@+id/LLL"
                                    android:fontFamily="@font/roboto"
                                    android:padding="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_5sdp"
                                    android:text="Once your friend download and placed
their 1st booking"
                                    android:textColor="#232323"

                                    android:textSize="@dimen/_12sdp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp">

                                <LinearLayout
                                    android:id="@+id/LLLL"
                                    android:layout_width="@dimen/_70sdp"
                                    android:layout_height="@dimen/_70sdp"
                                    android:background="@drawable/circleblue"
                                    android:gravity="center">

                                    <ImageView
                                        android:id="@+id/ivImagesss"
                                        android:layout_width="@dimen/_70sdp"
                                        android:layout_height="@dimen/_45sdp"
                                        android:layout_alignParentLeft="true"
                                        android:layout_gravity="center"
                                        android:backgroundTint="@color/white"
                                        android:padding="@dimen/_5sdp"
                                        android:src="@drawable/rupess"
                                        app:tint="@color/white" />
                                </LinearLayout>


                                <TextView
                                    android:id="@+id/choosefileee"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_toRightOf="@+id/LLLL"
                                    android:fontFamily="@font/roboto_bold"
                                    android:padding="@dimen/_5sdp"
                                    android:paddingLeft="@dimen/_5sdp"
                                    android:text="Both Get Paid"
                                    android:textColor="#232323"
                                    android:textSize="@dimen/_15sdp"
                                    android:textStyle="bold" />


                                <TextView
                                    android:id="@+id/newTexttt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/choosefileee"
                                    android:layout_toRightOf="@+id/LLLL"
                                    android:fontFamily="@font/roboto"

                                    android:paddingLeft="@dimen/_5sdp"
                                    android:text="@string/you_ll_get_paid_your_friend_get_too"
                                    android:textColor="#232323"
                                    android:textSize="@dimen/_12sdp" />
                            </RelativeLayout>
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </RelativeLayout>

            </RelativeLayout>


        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

    <ScrollView
        android:id="@+id/scroll_side_menu"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_topp"
        android:layout_alignParentLeft="true"
        android:animateLayoutChanges="true"
        android:background="@color/white"
        android:visibility="gone">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:layout_below="@+id/tvPackage"
            android:layout_margin="@dimen/_5sdp"
            android:padding="@dimen/_10sdp"
            app:cardBackgroundColor="@color/background"
            app:cardCornerRadius="@dimen/_12sdp"
            app:cardElevation="@dimen/_5sdp"
            app:contentPadding="0dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_2sdp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_Dashboard"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bl"
                    android:fontFamily="@font/lato"
                    android:padding="@dimen/_8sdp"
                    android:text="Dashboard"
                    android:textColor="@color/slider"
                    android:textSize="@dimen/_12sdp"
                    android:visibility="gone" />

                <TextView

                    android:id="@+id/tv_myBooking"
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:padding="@dimen/_8sdp"
                    android:text="My Booking"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp" />


                <TextView
                    android:id="@+id/tv_myProfile"

                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:padding="@dimen/_8sdp"
                    android:text="My Profile"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp" />

                <TextView

                    android:id="@+id/tv_myWallet"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="My Wallet"
                    android:textColor="@color/black_text"

                    android:textSize="@dimen/_12sdp" />

                <TextView

                    android:id="@+id/tv_changePassword"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="Change Password"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp" />

                <TextView

                    android:id="@+id/tv_myReviews"

                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"

                    android:background="@drawable/allsideborder"

                    android:fontFamily="@font/lato"
                    android:gravity="left"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="My Reviews &amp; Rating"
                    android:textColor="@color/black_text"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/tv_myAddressbook"


                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"

                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="My Address Book"
                    android:textColor="@color/black_text"
                    android:textSize="@dimen/_12sdp" />

                <TextView

                    android:id="@+id/tv_mySupport"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_margin="@dimen/_2sdp"
                    android:background="@drawable/allsideborder"
                    android:fontFamily="@font/lato"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="My Support"
                    android:textColor="@color/black_text"
                    android:textSize="@dimen/_12sdp" />

                <TextView

                    android:id="@+id/tv_referralDashboard"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_2sdp"

                    android:background="@color/head_lightBlue"
                    android:fontFamily="@font/lato"
                    android:gravity="left"
                    android:maxLines="1"
                    android:padding="@dimen/_8sdp"
                    android:text="Referral Dashboard"
                    android:textColor="@color/white"
